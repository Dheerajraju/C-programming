#include <stdio.h>

int main()
{
    int a[5] = {5, 2, 9, 1, 6};       // Initialize the array
    int n = sizeof(a) / sizeof(a[0]); // Find number of elements
    int i, j, temp;

    // Print array before sorting
    printf("Array before sorting: ");
    for(i = 0; i < n; i++)
        printf("%d ", a[i]);
    printf("\n");

    // Bubble sort logic
    for(i = 0; i < n - 1; i++)            // Outer loop (n-1 passes)
    {
        for(j = 0; j < n - 1 - i; j++)    // Inner loop (compare adjacent elements)
        {
            if(a[j] > a[j + 1])           // Swap if elements are in wrong order
            {
                temp = a[j];
                a[j] = a[j + 1];
                a[j + 1] = temp;
            }
        }
    }

    // Print array after sorting
    printf("Array after sorting: ");
    for(i = 0; i < n; i++)
        printf("%d ", a[i]);
    printf("\n");

    return 0;
}
